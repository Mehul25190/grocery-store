{
  "expo": {
    "name": "MyAllaadin",
    "slug": "myallaadin",
    "description": "My Allaadin is a grocery delivery service where personal shoppers select your items from the store and then deliver them to your door.",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#D7ECDD"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "app/assets/*"
    ],
    "android": {
      "package": "com.myallaadin.store",
      "versionCode": 2,
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.myallaadin"
    }
  }
}
